{
  "name": "bitcoin-price-ticker",
  "manifest_version": 2,
  "version": "12",

  "description": "Adds price tickers for Bitcoin and many other cryptocurrencies",
  "homepage_url": "https://sebaslogen.github.io/bitcoin-price-ticker/",
  "icons": {
    "48": "icon.png"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "browser_style": true,
    "default_icon": "icon.png",
    "default_title": "button label"
  }
}